{
  "tags": {
    "dictionaries": ["jsdoc","closure"]
  },
  "plugins": ["plugins/markdown"],
  "source": {
    "include": ["./"],
    "includePattern": ".+\\.(js)$",
    "excludePattern": "(node_modules/|docs)"
  },
  "opts": {
    "destination": "./docs",
    "template": "node_modules/minami",
    "readme": "README.md",
    "recurse": true,
    "package": "package.json"
  }
}
